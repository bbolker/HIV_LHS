## options(repos=c(CRAN="http://probability.ca/cran/"))
module unload intel; module load r/3.2.3
BATCHSIZE=20
for BATCHNUM in {0..49}; do
     echo $BATCHNUM
     fn=ev_LHSfull_batch${BATCHNUM}
     sqsub -q serial -o $fn.%J.out --mpp 4G -r 36h R CMD BATCH --vanilla "--args $BATCHNUM $BATCHSIZE" ev_LHSfull_chunk.R $fn.Rout
done
